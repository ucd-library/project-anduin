{
  "registry" : "us-west1-docker.pkg.dev/digital-ucdavis-edu/pub",

  "repositories" : {
    "postgres" : "https://github.com/ucd-library/postgres"
  },

  "images" : {
    "dagster" : {
      "contextPath" : "dagster"
    },
    "superset" : {
      "contextPath" : "superset"
    },
    "auth-gateway" : {
      "contextPath" : "auth-gateway"
    },
    "anduin-pg" : {
      "contextPath" : "postgres",
      "options" : {
        "build-arg" : "POSTGRES_IMAGE=${postgres.postgres}"
      }
    }
  }
}